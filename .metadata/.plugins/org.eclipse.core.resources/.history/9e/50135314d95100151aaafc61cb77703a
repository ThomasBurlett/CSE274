public class ParenMatcher {
	static public boolean ParenMatch(String s) {
		IntStack stack = new IntStack(s.length());
		char[] c = s.toCharArray();
		
		for(char l: c) {
			if("(".indexOf(l) >= 0) {
				stack.push(10);
			} else {
				if (l == ')') {
					if(!(stack.pop() == 10)) {
						return false;
					}
				}
				
			}
		}
	
		
	}
}
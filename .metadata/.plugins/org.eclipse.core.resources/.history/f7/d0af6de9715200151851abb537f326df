public class ParenMatcher {
	static public boolean ParenMatch(String s) {

		char[] letters = s.toCharArray();
		IntStack stack = new IntStack(s.length());

		for (char letter : letters) {
			System.out.println(letter);
			if ("(".indexOf(letter) >= 0) {
				stack.push(10);
			} else if (stack.isEmpty()) {
				if(letter == 1) {
					return true;
				}
				return false;
			} else if (letter == ')') {
				if (!(stack.pop() == 10)) {
					return false;
				}
			}
		}
		
		if (stack.size() != 0) {
			return false;
		}
		return true;

	}
}

